{
  "name": "88mph",
  "license": "GPL-3.0-or-above",
  "scripts": {
    "codegen": "npx graph codegen",
    "build": "npx graph build",
    "deploy": "npx graph deploy --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ bacon-labs/eighty-eight-mph-v3-polygon",
    "create-local": "npx graph create --node http://localhost:8020/ bacon-labs/eighty-eight-mph-v3-polygon",
    "remove-local": "npx graph remove --node http://localhost:8020/ bacon-labs/eighty-eight-mph-v3-polygon",
    "deploy-local": "npx graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 bacon-labs/eighty-eight-mph-v3-polygon",
    "prettier": "npx prettier --write '**/*.json' '**/*.ts' '**/*.yaml' '**/*.graphql'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.21.1",
    "@graphprotocol/graph-ts": "^0.20.1"
  },
  "devDependencies": {
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^1.19.1"
  },
  "lint-staged": {
    "*.{json,ts,yaml,graphql}": "prettier --write"
  }
}
